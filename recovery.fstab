/boot			emmc	/dev/block/bootdevice/by-name/boot
/system			ext4	/dev/block/bootdevice/by-name/system
/system_image	emmc	/dev/block/bootdevice/by-name/system
/cache			ext4	/dev/block/bootdevice/by-name/cache
/misc			emmc	/dev/block/bootdevice/by-name/misc
/keymaster      emmc    /dev/block/bootdevice/by-name/keymaster
/keystore		emmc	/dev/block/bootdevice/by-name/keymaster
/recovery		emmc	/dev/block/bootdevice/by-name/recovery		flags=backup=1
/data			ext4	/dev/block/bootdevice/by-name/userdata		flags=encryptable=/dev/block/bootdevice/by-name/encrypt
/efs1			emmc	/dev/block/bootdevice/by-name/modemst1		flags=backup=1;display="EFS"
/efs2			emmc	/dev/block/bootdevice/by-name/modemst2		flags=backup=1;subpartitionof=/efs1
/efsg			emmc	/dev/block/bootdevice/by-name/fsg			flags=backup=1;subpartitionof=/efs1
/modem			emmc	/dev/block/bootdevice/by-name/modem			flags=display="Modem";flashimg
/firmware		vfat	/dev/block/bootdevice/by-name/modem			flags=mounttodecrypt;backup=1;display="Firmware";fsflags=context=u:object_r:firmware_file:s0
/persist-lg		ext4	/dev/block/bootdevice/by-name/drm 			flags=mounttodecrypt;display="Persist-LG"
/persist		ext4	/dev/block/bootdevice/by-name/persist		flags=display="Persist"
/external_sd	vfat	/dev/block/mmcblk0p1 /dev/block/mmcblk0		flags=display="mSDcard";storage;wipeingui;removable;encryptable=/dev/block/bootdevice/by-name/encrypt
/usb-otg		vfat	/dev/block/sdh1 /dev/block/sdh				flags=display="USB-OTG";storage;wipeingui;removable
/OP				ext4	/dev/block/bootdevice/by-name/OP			flags=display="OP Configs"
